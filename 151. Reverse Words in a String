class Solution(object):
    def isSubsequence(self, s, t):
        """
        method: two pointer 
        set two pointers for the two string 
        have a while loop to make sure the index dont go out of bound:
            if the two pointer charter equal:
                move the s pointer so we looking for next charter in t 
            move the t pointer 
        
        if the s is empty than return true we found all charater 
        else false        
        """
        i =0
        j =0
        while i < len(s) and j< len(t):
            if s[i] == t[j]:
                i += 1
            j+=1
        
        if i == len(s):
            return True 
        else:
            False
